#include <iostream>
using namespace std;

int main(){
    int n,m,zbirM=0,num=0;

    cin>>n;

    for (int i=n-1; i>0; i--) {
        int zbir=0;
        for (int j=1; j<i; j++) {
            if (i%j==0) {
                zbir+=j;
            }
        }

        if (zbir>zbirM) {
            zbirM=zbir;
            num=i;
        }

    }

    cout<<num;
    return 0;
}
