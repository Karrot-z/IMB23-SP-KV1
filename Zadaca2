#include <iostream>
using namespace std;

int pow(int a) {
    int n=1;
    for (int i=1; i<a; i++)
        n*=10;
    return n;
}

int main(){
    int n,nminus1;;
    cin>>n;

    if (n<9) {
        cout<<"Brojot ne e validen";
        return 0;
    }

    for (int j=n-1; j>0; j--) {
        nminus1=0;
        int nj = j;
        int nm2=j, d2=0;

        while (nm2>0) {
            nm2/=10;
            d2++;
        }
        for (int i=d2; i>0; i--) {
            int dig = nj%10;
            nj/=10;
            nminus1+=dig*pow(i);
        }
        
        if (nminus1 > 0 && nminus1%d2==0) {
            cout<<j<<endl;
            break;
        }
    }


    return 0;
}
