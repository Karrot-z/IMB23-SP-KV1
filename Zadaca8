#include <iostream>
using namespace std;

int main(){
    int a,b,a2,b2;
    cin>>a>>b;


    for (int i=a-1; i<=a; i--) {
        b2=b;
        int k;
        while (b2>0) {
            int currentCifra=b2%10;
            b2/=10;
            a2=i;
            k=0;
            while (a2>0) {
                int currentCifraA=a2%10;
                a2/=10;
                if (currentCifraA==currentCifra) {
                    k++;
                    break;
                }
            }
            if (k==1) {
                break;
            }
        }
        if (k==0) {
            cout<<i;
            return 0;
        }

    }


    return 0;
}
